<main>
    <div class="display-1 text-center">
        {{page_header.title}}
    </div>
    <div class="text-center text-uppercase">
        {{page_header.description}}
    </div>
    <div class="card">
        <div class="card-body">
            <div class="display-6 text-end">{{section.about_us.title_list.main}}</div>
            <div class="display-2 text-end">{{section.about_us.title_list.sub}}</div>
            <button class="btn btn-outline-info">Show more</button>
        </div>
    </div>
    <br>

    <div class="card">
        <div class="card-body">
            <div class="display-5">Advantage</div>
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{section.advantage.efficiency.title}}</div>
                    <br>
                    <div class="display-6">{{section.advantage.efficiency.sub_title}}</div>
                    <br>
                    <div>{{section.advantage.efficiency.description}}</div>
                    <br>
                    <button class="btn btn-outline-warning">Show more</button>
                </div>
            </div>
            <br>

            <div class="card">
                <div class="card-body">
                    <div class="display-4 text-center">{{section.advantage.innovation.title}}</div>
                    <div class="display-6 text-center">{{section.advantage.innovation.sub_title}}</div>
                    <div class="text-center">{{section.advantage.innovation.description}}</div>
                    <button class="btn btn-outline-warning">Show more</button>
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="display-4 text-end">{{section.advantage.collaboration.title}}</div>
                    <div class="display-6 text-end">{{section.advantage.collaboration.sub_title}}</div>
                    <div class="text-end">{{section.advantage.collaboration.description}}</div>
                    <button class="btn btn-outline-warning">Show more</button>
                </div>
            </div>
        </div>
    </div>
    <div class="card">
        <div class="card-body">
            <div class="display-5">{{section.agreement.title}}</div>
            <div class="card">
                <div class="card-body">
                    <div class="display-5">{{section.agreement.info.sub_title}}</div>
                    <div>{{section.agreement.info.detail.text}}</div>
                    <button class="btn btn-success">Show more</button>
                </div>
            </div>
        </div>
    </div>
</main>